{
  "version": 2,
  "builds": [
    {
      "src": "ogsp/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ogsp/frontend/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://project-bj5p.onrender.com"
  }
}